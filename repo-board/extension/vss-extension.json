{
    "manifestVersion": 1,
    "id": "repo-board",
    "publisher": "ovantol",
    "version": "0.0.5",
    "name": "repo-board",
    "description": "Git repository overview for Azure DevOps",
    "demands": ["api-version/5.0"],
    "scopes": ["vso.code_manage"],
    "public": false,
    "categories": [
        "Azure Repos"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/repo-board.png"
    },
    "contributions": [
        {
            "id": "my-hub",
            "type": "ms.vss-web.hub",
        
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "RepoBoard",
                "icon": "images/repo-board.png",
                "uri": "app/index.html"
            }
        }
    ],
    "files": [
        {
            "path": "app",
            "addressable": true,
            "packagePath": "app"
        }       
    ]
}